{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mobile",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.makenti22.food"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "usesCleartextTraffic": true,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "CAMERA"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.makenti22.food",
      "networkSecurityConfig": "./network_security_config.xml"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Приложению требуется доступ к камере для сканирования QR кодов."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ee563766-ca8e-45e3-9cd7-1933d5f2813b"
      },
      "env": {
        "API_BASE_URL": "https://gembos.ru",
        "API_TIMEOUT": "10000",
        "AUTH_TOKEN_KEY": "auth_token",
        "REFRESH_TOKEN_KEY": "refresh_token",
        "APP_NAME": "Food Link",
        "APP_VERSION": "0.0.1",
        "ENABLE_LOGGING": "true",
        "LOG_LEVEL": "debug",
        "CACHE_TIMEOUT": "300000",
        "ENABLE_ANIMATIONS": "true",
        "ANIMATION_DURATION": "300",
        "NODE_ENV": "development",
        "S3_BUCKET_PREFIX": "https://gembos.ru/images/food-link-images"
      }
    }
  }
}
