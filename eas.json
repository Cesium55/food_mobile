{
  "cli": {
    "version": ">= 16.21.0",
    "appVersionSource": "remote"
  },
  "build": {
    "local": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://gembos.ru",
        "EXPO_PUBLIC_API_TIMEOUT": "10000",
        "EXPO_PUBLIC_AUTH_TOKEN_KEY": "auth_token",
        "EXPO_PUBLIC_REFRESH_TOKEN_KEY": "refresh_token",
        "EXPO_PUBLIC_APP_NAME": "Food Link",
        "EXPO_PUBLIC_APP_VERSION": "1.0.0",
        "EXPO_PUBLIC_ENABLE_LOGGING": "true",
        "EXPO_PUBLIC_LOG_LEVEL": "debug",
        "EXPO_PUBLIC_CACHE_TIMEOUT": "300000",
        "EXPO_PUBLIC_ENABLE_ANIMATIONS": "true",
        "EXPO_PUBLIC_ANIMATION_DURATION": "300",
        "EXPO_PUBLIC_NODE_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://gembos.ru",
        "EXPO_PUBLIC_API_TIMEOUT": "15000",
        "EXPO_PUBLIC_AUTH_TOKEN_KEY": "auth_token_staging",
        "EXPO_PUBLIC_REFRESH_TOKEN_KEY": "refresh_token_staging",
        "EXPO_PUBLIC_APP_NAME": "Food Link (Staging)",
        "EXPO_PUBLIC_APP_VERSION": "1.0.0-staging",
        "EXPO_PUBLIC_ENABLE_LOGGING": "true",
        "EXPO_PUBLIC_LOG_LEVEL": "info",
        "EXPO_PUBLIC_CACHE_TIMEOUT": "600000",
        "EXPO_PUBLIC_ENABLE_ANIMATIONS": "true",
        "EXPO_PUBLIC_ANIMATION_DURATION": "300",
        "EXPO_PUBLIC_NODE_ENV": "staging"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://gembos.ru",
        "EXPO_PUBLIC_API_TIMEOUT": "20000",
        "EXPO_PUBLIC_AUTH_TOKEN_KEY": "auth_token",
        "EXPO_PUBLIC_REFRESH_TOKEN_KEY": "refresh_token",
        "EXPO_PUBLIC_APP_NAME": "Food Link",
        "EXPO_PUBLIC_APP_VERSION": "1.0.0",
        "EXPO_PUBLIC_ENABLE_LOGGING": "false",
        "EXPO_PUBLIC_LOG_LEVEL": "error",
        "EXPO_PUBLIC_CACHE_TIMEOUT": "1800000",
        "EXPO_PUBLIC_ENABLE_ANIMATIONS": "true",
        "EXPO_PUBLIC_ANIMATION_DURATION": "250",
        "EXPO_PUBLIC_NODE_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
